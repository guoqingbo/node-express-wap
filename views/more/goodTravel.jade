extends ../layout

block stylesheets
    link(rel="stylesheet", href="/stylesheets/xcy/lookMore.css")
block javascripts

block content
    a.back-bar(href='javascript:window.history.back(-1);')
    .look-more
        ul
            each item in data
                li.look-more-item
                    a(href="/more/goodTravel/" + item.baseCode)
                        .title-name=item.name
                        .img-box
                            img(src=item.img,alt="图片")
                        .bottom
                            i.icon.iconfont.icon-xin
                            span= item.careNum


